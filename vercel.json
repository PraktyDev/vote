{ 
    "version": 2,
    "name": "vote machine",
    "routes": [
        { 
            "src": "/(.*)",
            "dest": "/vote.js"
        }
    ],
    "builds": [
        { 
            "src": "/vote.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "vote.js": {
          "memory": 1024,
          "maxDuration": 10
        }
    }
}
  